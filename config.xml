<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="1.0.0" xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Coupon</name>
    <description>
        Sydney Coupon
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        UTS Studio
    </author>
    <content src="index.html"/>
    <preference value="default" name="Orientation"/>
    <platform name="ios">
    <preference value="all" name="Orientation"/>
    </platform>
    <preference value="default" name="StatusBarStyle"/>
    <preference value="false" name="ShowSplashScreenSpinner"/>
    <preference value="false" name="AutoHideSplashScreen"/>
    <preference value="true" name="DisallowOverscroll"/>

    <plugin name="com.indigoway.cordova.whitelist.whitelistplugin" source="pgb" spec="1.1.1"/>
    <plugin name="cordova-plugin-splashscreen" spec="~2.1.0"/>
    <plugin name="net.roughshod.plugins.barcodescanner" source="pgb" spec="3.0.0"/>
    <plugin name="cordova-plugin-battery-status" spec="~1.1.0"/>
    <plugin name="cordova-plugin-camera" spec="~1.2.0"/>
    <plugin name="cordova-plugin-console" spec="~1.0.1"/>
    <plugin name="cordova-plugin-contacts" spec="~1.1.0"/>
    <plugin name="cordova-plugin-device" spec="~1.0.1"/>
    <plugin name="cordova-plugin-device-motion" spec="~1.1.1"/>
    <plugin name="cordova-plugin-device-orientation" spec="~1.0.1"/>
    <plugin name="cordova-plugin-dialogs" spec="~1.1.1"/>
    <plugin name="cordova-plugin-file" spec="~3.0.0"/>
    <plugin name="cordova-plugin-file-transfer" spec="~1.3.0"/>
    <plugin name="cordova-plugin-geolocation" spec="~1.0.1"/>
    <plugin name="cordova-plugin-globalization" spec="~1.0.1"/>
    <plugin name="cordova-plugin-inappbrowser" spec="~1.0.1"/>
    <plugin name="cordova-plugin-media" spec="~1.0.1"/>
    <plugin name="cordova-plugin-media-capture" spec="~1.0.1"/>
    <plugin name="cordova-plugin-network-information" spec="~1.0.1"/>
    <plugin name="cordova-plugin-splashscreen" spec="~2.1.0"/>
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1"/>
    <plugin name="cordova-plugin-vibration" spec="~1.2.0"/>
    <plugin name="phonegap-plugin-contentsync" spec="~1.2.2"/>
    <plugin name="cordova-plugin-whitelist" spec="~1.0.0"/>
    <plugin name="adobe-mobile-services" spec="^4.7.0"/>
    <plugin name="nl.x-services.plugins.insomnia" spec="https://github.com/EddyVerbruggen/Insomnia-PhoneGap-Plugin.git#:."/>

    <plugin name="cordova-plugin-ble-central" spec="~1.0.1"/>

    <icon src="icon.png"/>
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/res/icon/android/icon-36-ldpi.png"/>
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/res/icon/android/icon-48-mdpi.png"/>
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/res/icon/android/icon-72-hdpi.png"/>
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/res/icon/android/icon-96-xhdpi.png"/>

    <icon gap:platform="ios" height="57" src="www/res/icon/ios/icon-57.png" width="57"/>
    <icon gap:platform="ios" height="72" src="www/res/icon/ios/icon-72.png" width="72"/>
    <icon gap:platform="ios" height="114" src="www/res/icon/ios/icon-57-2x.png" width="114"/>
    <icon gap:platform="ios" height="144" src="www/res/icon/ios/icon-72-2x.png" width="144"/>
    <icon gap:platform="webos" src="www/res/icon/webos/icon-64.png"/>

    <platform name="ios">
        <splash height="480" src="www/res/splash/ios/Default~iphone.png" width="320"/>
        <splash height="960" src="www/res/splash/ios/Default@2x~iphone.png" width="640"/>
        <splash height="1024" src="www/res/splash/ios/Default-Portrait~ipad.png" width="768"/>
        <splash height="2048" src="www/res/splash/ios/Default-Portrait@2x~ipad.png" width="1536"/>
        <splash height="768" src="www/res/splash/ios/Default-Landscape~ipad.png" width="1024"/>
        <splash height="1536" src="www/res/splash/ios/Default-Landscape@2x~ipad.png" width="2048"/>
        <splash height="1136" src="www/res/splash/ios/Default-568h@2x~iphone.png" width="640"/>
        <splash height="1334" src="www/res/splash/ios/Default-667h.png" width="750"/>
        <splash height="2208" src="www/res/splash/ios/Default-736h.png" width="1242"/>
        <splash height="1242" src="www/res/splash/ios/Default-Landscape-736h.png" width="2208"/>
    </platform>
    <platform name="android">
        <preference name="SplashScreenDelay" value="0"/>
        <icon density="ldpi" src="www/res/icon/android/icon-36.png"/>
        <icon density="mdpi" src="www/res/icon/android/icon-48.png"/>
        <icon density="hdpi" src="www/res/icon/android/icon-72.png"/>
        <icon density="xhdpi" src="www/res/icon/android/icon-96.png"/>
    </platform>

    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>

    <engine name="wp8" spec="~3.8.1"/>
    <engine name="ios" spec="~3.9.2"/>
    <engine name="android" spec="~4.1.1"/>
</widget>
